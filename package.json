{
  "name": "@yourusername/ai-hrms",
  "version": "1.0.2",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "vite": "^5.0.10"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "client": "cd ai-hrms/client && npm run dev",
    "server": "cd ai-hrms/server && npm run dev",
    "start": "concurrently \"npm run client\" \"npm run server\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "HRMS",
    "AI",
    "MERN",
    "Human Resources",
    "Payroll",
    "Attendance",
    "Vite",
    "React",
    "Tailwind"
  ],
  "dependencies": {
    "concurrently": "^8.2.2",
    "next": "^15.1.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
